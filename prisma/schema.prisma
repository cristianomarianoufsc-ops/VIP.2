generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")

}

model Image {
  id        String   @id @default(cuid())
  shortId   String   @unique
  fileName  String
  imageUrl  String
  shortUrl  String
  createdAt DateTime @default(now())
}
