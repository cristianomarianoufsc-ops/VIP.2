datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Image {
  id        String   @id @default(cuid())
  shortId   String   @unique @default(cuid())
  fileName  String
  fileKey   String   @unique
  imageUrl  String
  createdAt DateTime @default(now())
}
