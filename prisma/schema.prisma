datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Image {
  id        String     @id @default(cuid())
  shortId   String     @unique @default(cuid())
  fileName  String
  fileKey   String     @unique
  imageUrl  String
  createdAt DateTime   @default(now())
}
